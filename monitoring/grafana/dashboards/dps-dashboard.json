{
  "dashboard": {
    "id": null,
    "title": "Deep Parallel Synthesis - System Overview",
    "tags": ["dps", "ai", "reasoning"],
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "API Request Rate",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 100},
                {"color": "red", "value": 500}
              ]
            }
          }
        },
        "targets": [
          {
            "expr": "sum(rate(dps_api_requests_total[5m]))",
            "legendFormat": "Requests/sec"
          }
        ]
      },
      {
        "id": 2,
        "title": "Response Time",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0},
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(dps_api_request_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "95th percentile"
          }
        ]
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0},
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        },
        "targets": [
          {
            "expr": "sum(rate(dps_api_requests_total{status=~\"4..|5..\"}[5m])) / sum(rate(dps_api_requests_total[5m])) * 100",
            "legendFormat": "Error Rate %"
          }
        ]
      },
      {
        "id": 4,
        "title": "Active Reasoning Tasks",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "red", "value": 100}
              ]
            }
          }
        },
        "targets": [
          {
            "expr": "dps_api_active_connections",
            "legendFormat": "Active Tasks"
          }
        ]
      },
      {
        "id": 5,
        "title": "Reasoning Performance Over Time",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(dps_api_request_duration_seconds_bucket{endpoint=\"/reason\"}[5m])) by (le))",
            "legendFormat": "P50 Response Time"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(dps_api_request_duration_seconds_bucket{endpoint=\"/reason\"}[5m])) by (le))",
            "legendFormat": "P95 Response Time"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(dps_api_request_duration_seconds_bucket{endpoint=\"/reason\"}[5m])) by (le))",
            "legendFormat": "P99 Response Time"
          }
        ]
      },
      {
        "id": 6,
        "title": "Model Load Times",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(dps_model_load_time_seconds_bucket[5m])) by (le))",
            "legendFormat": "Model Load Time P95"
          }
        ]
      },
      {
        "id": 7,
        "title": "System Resource Usage",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 16},
        "fieldConfig": {
          "defaults": {
            "unit": "percent"
          }
        },
        "targets": [
          {
            "expr": "avg(rate(container_cpu_usage_seconds_total{namespace=\"dps-system\"}[5m])) * 100",
            "legendFormat": "CPU Usage %"
          },
          {
            "expr": "avg(container_memory_usage_bytes{namespace=\"dps-system\"}) / avg(container_spec_memory_limit_bytes{namespace=\"dps-system\"}) * 100",
            "legendFormat": "Memory Usage %"
          }
        ]
      },
      {
        "id": 8,
        "title": "Pod Status",
        "type": "stat",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 16},
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "green", "value": 3}
              ]
            }
          }
        },
        "targets": [
          {
            "expr": "sum(kube_pod_status_ready{namespace=\"dps-system\", condition=\"true\"})",
            "legendFormat": "Ready Pods"
          }
        ]
      },
      {
        "id": 9,
        "title": "Database Connections",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 16},
        "targets": [
          {
            "expr": "mongodb_connections{state=\"current\"}",
            "legendFormat": "MongoDB Connections"
          },
          {
            "expr": "redis_connected_clients",
            "legendFormat": "Redis Connections"
          }
        ]
      },
      {
        "id": 10,
        "title": "Reasoning Chain Quality Metrics",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 1
          }
        },
        "targets": [
          {
            "expr": "avg(dps_reasoning_confidence)",
            "legendFormat": "Average Confidence"
          },
          {
            "expr": "avg(dps_synthesis_quality)",
            "legendFormat": "Synthesis Quality"
          }
        ]
      },
      {
        "id": 11,
        "title": "Top Failing Endpoints",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "fieldConfig": {
          "defaults": {
            "custom": {
              "displayMode": "list"
            }
          }
        },
        "targets": [
          {
            "expr": "topk(5, sum by (endpoint) (rate(dps_api_requests_total{status=~\"4..|5..\"}[5m])))",
            "format": "table",
            "instant": true
          }
        ]
      }
    ],
    "templating": {
      "list": [
        {
          "name": "namespace",
          "type": "query",
          "query": "label_values(dps_api_requests_total, namespace)",
          "current": {
            "value": "dps-system",
            "text": "dps-system"
          },
          "options": [],
          "refresh": 1
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "expr": "changes(kube_deployment_status_replicas{namespace=\"$namespace\"}[1m]) > 0",
          "textFormat": "Deployment: {{deployment}}",
          "tags": ["deployment"]
        }
      ]
    }
  }
}